/*
 * 基准样式，相当于 reset.css，使主题在不同平台下有统一的表现。
 * 各个属性的默认值为 chrome 的默认样式，见 variables/default.scss。
 */
:root {
  --text-color: #1f2329;
  --bg-color: white;
  --side-bar-bg-color: white;
  --active-file-bg-color: white;
  --rawblock-edit-panel-bd: #f5f6f7;
  --window-border: 0 solid white;
  --control-text-color: #1f2329;
  --primary-color: #3370ff;
  --primary-btn-border-color: #3370ff;
  --active-file-border-color: #3370ff;
  --primary-btn-text-color: #3370ff;
  --item-hover-text-color: #3370ff;
  --meta-content-color: #3370ff;
  --search-select-text-color: #3370ff;
  --heading-char-color: #3370ff;
  /* Mermaid diagrams*/
  --mermaid-theme: default;
}

 * {
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  margin:0;
  padding:0;
}
body{
    font-size: 16px;
    color: #282c34;
}
h1, h2, h3, h4 {
    color: #282c34;
    font-weight: 400;
}

h1, h2, h3, h4, h5 {
    font-weight:500;
}
h1, h2, h3, h4, h5, dl{
    padding: 0;
}

p {
    display: block;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    line-height: 1.7;
}
h2 {
    padding-top:64px;
    font-size: 36px;
    padding-bottom: .3rem;
}
h3 {
    padding-top:64px;
    font-size: 24px;
    line-height: 30px;
    padding-bottom: .3rem;
    margin: -28px 0 12px;
}
h4 {
    padding-top:64px;
    font-size: 21px;
    line-height: 26px;
    margin: -40px 0 8px;
}
h5 {
    padding-top:64px;
    font-size: 18px;
    line-height: 23px;
    margin: -43px 0 6px;
}
h1 {
    padding-top:64px;
    padding-bottom: 2rem;
    border-bottom: 1px solid #e5e8ef;
}
 pre,  xmp,  plaintext,  listing {
  font-family: var(--monospace);
}
 hr {
  border-style: inset;
  border-width: 1px;
}
 img {
  max-width: 100%;
  height: auto;
}
ul {
    list-style-type: disc;
}
ol, p, ul {
    line-height: 1.7;
}
 ul li {
    list-style:disc;
  display: list-item;
    text-align: -webkit-match-parent;
}
ol li {
    list-style:decimal;
}
 li + li {
  margin-top: 4px;
}
 ul ul,  ol ul,  ul ol,  ol ol {
  margin-top: 0;
  margin-bottom: 0;
}
 .task-list-item {
  list-style-type: none;
}
 a img {
  border: none;
}
 b,  strong {
  font-weight: bold;
}
 i,  cite,  em,  var,  address,  dfn {
  font-style: italic;
}
 code,  kbd,  tt,  samp {
  font-family: var(--monospace);
}
 mark {
  color: #1f2329;
  padding: 0.2em 0;
  background-color: rgba(255, 246, 122, 0.8);
}
 del,  s {
  color: inherit;
}
 table {
  color: black;
  border-collapse: collapse;
  background-color: white;
  border-spacing: 2px;
  font-size: 1em;
}
 th,  td {
  border-color: #dee0e3;
  border-width: 1px;
  border-style: solid;
}
 th {
  padding: 8px 8px;
  font-size: 1em;
  font-weight: 500;
}
 td {
  padding: 8px 8px;
  font-size: 1em;
  font-weight: normal;
}
 pre.md-fences,
 pre.md-meta-block,
 .md-rawblock-control:not(.md-rawblock-tooltip) .md-rawblock-input {
  border-radius: 3px;
  border: 1px solid #dee0e3;
  padding: 10px;
  background-color: #f5f6f7;
}
 .CodeMirror div.CodeMirror-cursor {
  border-color: #1f2329;
}
 .md-math-block,  .md-rawblock {
  margin-top: 1em;
  margin-bottom: 1em;
}
 .md-rawblock > .md-rawblock-tooltip {
  background-color: #f5f6f7;
  transition: 0.5s;
  display: block;
  visibility: hidden;
  opacity: 0;
}
 .md-rawblock:hover > .md-rawblock-tooltip {
  transition: 0.5s;
  visibility: visible;
  opacity: 1;
  animation: none;
}
 .md-rawblock-control:not(.md-rawblock-tooltip) {
  padding-left: 0px;
  padding-right: 0px;
}

 * {
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
}
ul li {
    unicode-bidi: isolate;
    font-variant-numeric: tabular-nums;
    text-transform: none;
    text-indent: 0px !important;
    text-align: start !important;
    text-align-last: start !important;
}
ol li::marker {
    unicode-bidi: isolate;
    font-variant-numeric: tabular-nums;
    text-transform: none;
    text-indent: 0px !important;
    text-align: start !important;
    text-align-last: start !important;
}
 blockquote {
  display: block;
  margin-left: 0;
  padding-left: 14px;
  border-left: 2px solid #3370ff;
  background: #f1f1f1;

}
code {
    color: #476582;
    padding: .25rem .5rem;
    margin: 0;
    font-size: .85em;
    background-color: rgba(27,31,35,.05);
    border-radius: 3px;
}
a {
    color:black;
    margin: 0 2px;
    padding: 0;
    vertical-align: baseline;
    text-decoration: none;
}
.inner-content a{
  color: #417ff9 ;
}
a.header-anchor{
    float:left;
    margin-left: -.87em;
}
a:hover {
    text-decoration: none;
    color: #417ff9;
    transition: all .2s linear 0s;
}
a:visited {
    /*color: purple;*/
}
.header-anchor{
    opacity:0;
    transition: all .2s linear 0s;
}
.header-anchor:hover{
    opacity:1;
}
 hr {
  margin: 13px 0 12px;
  border: 0px;
  height: 1px;
  background-origin: content-box;
  background-image: linear-gradient(90deg, #bbbfc4, #bbbfc4);
  background-repeat: no-repeat;
}
 ul {
  margin-left: 0;
  padding-left: 22px;
  list-style: none;
}
 ul > li:not([class*=task-list-item]) {
  word-wrap: break-all;
  padding-left: 0px;
}
 ol {
  counter-reset: ol_counter;
  margin-left: 0;
  padding-left: 22px;
  list-style: none;
}
 li + li,  ul ul,  ol ul,  ul ol,  ol ol,  li ul,  li ol {
  margin-top: 4px;
}
 strong {
  color: rgb(84,125,246);
}